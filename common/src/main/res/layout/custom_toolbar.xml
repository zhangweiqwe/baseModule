<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/toolbar_cl"
    android:layout_width="match_parent"
    android:layout_height="?android:attr/actionBarSize">


    <ViewStub
        android:id="@+id/toolbar_back_vs"
        android:layout_width="wrap_content"
        android:layout_height="?android:attr/actionBarSize"
        android:inflatedId="@+id/toolbar_back_iv"
        android:layout="@layout/toolbar_left_back_item"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




    <ViewStub
        android:id="@+id/toolbar_title_vs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/toolbar_title_tv"
        android:layout="@layout/toolbar_center_title_item"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ViewStub
        android:id="@+id/toolbar_right_multiple_menu_vs"
        android:layout_width="wrap_content"
        android:layout_height="?android:attr/actionBarSize"
        android:inflatedId="@+id/toolbar_right_multiple_menu_ll"
        android:layout="@layout/toolbar_right_multiple_menu_item"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<cn.wsgwz.basemodule.widgets.progressActivity.ProgressConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/progress_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".BaseNetworkActivity">

    <ViewStub android:id="@+id/toolbar_stub"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:inflatedId="@+id/toolbar_stub"
        android:layout="@layout/view_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <FrameLayout
        android:id="@+id/content"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_stub"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>


</cn.wsgwz.basemodule.widgets.progressActivity.ProgressConstraintLayout>